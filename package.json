{
  "name": "@nextcloud/dialogs",
  "version": "7.1.0",
  "description": "Nextcloud dialog helpers",
  "keywords": [
    "nextcloud"
  ],
  "homepage": "https://github.com/nextcloud-libraries/nextcloud-dialogs#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/nextcloud-libraries/nextcloud-dialogs"
  },
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite --mode production build",
    "build:doc": "npm run dev && npm run doc",
    "dev": "vite --mode development build",
    "doc": "typedoc --tsconfig tsconfig-typedoc.json --highlightLanguages vue --plugin typedoc-plugin-missing-exports --out dist/doc dist/index.d.ts dist/filepicker.d.ts && touch dist/doc/.nojekyll",
    "l10n:extract": "node build/extract-l10n.js",
    "lint": "eslint lib/",
    "lint:fix": "eslint --fix lib/",
    "prerelease:format-changelog": "node build/format-changelog.mjs",
    "stylelint": "stylelint lib/**/*.vue",
    "stylelint:fix": "stylelint lib/**/*.vue --fix",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "ts:check": "vue-tsc --noEmit",
    "watch": "vite --mode development build --watch"
  },
  "browserslist": [
    "extends @nextcloud/browserslist-config"
  ],
  "stylelint": {
    "extends": "@nextcloud/stylelint-config"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@nextcloud/auth": "^2.5.3",
    "@nextcloud/axios": "^2.5.2",
    "@nextcloud/browser-storage": "^0.5.0",
    "@nextcloud/event-bus": "^3.3.3",
    "@nextcloud/files": "^3.12.2",
    "@nextcloud/initial-state": "^3.0.0",
    "@nextcloud/l10n": "^3.4.1",
    "@nextcloud/paths": "^2.4.0",
    "@nextcloud/router": "^3.1.0",
    "@nextcloud/sharing": "^0.3.0",
    "@nextcloud/vue": "^9.3.0",
    "@types/toastify-js": "^1.12.4",
    "@vueuse/core": "^14.1.0",
    "cancelable-promise": "^4.3.1",
    "p-queue": "^9.0.1",
    "toastify-js": "^1.12.0",
    "vue": "^3.5.25",
    "webdav": "^5.8.0"
  },
  "devDependencies": {
    "@mdi/svg": "^7.4.47",
    "@nextcloud/browserslist-config": "^3.1.2",
    "@nextcloud/eslint-config": "^9.0.0-rc.6",
    "@nextcloud/logger": "^3.0.3",
    "@nextcloud/stylelint-config": "^3.1.1",
    "@nextcloud/vite-config": "^2.5.2",
    "@testing-library/vue": "^8.1.0",
    "@types/gettext-parser": "^8.0.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "core-js": "^3.47.0",
    "gettext-extractor": "^4.0.3",
    "gettext-parser": "^8.0.0",
    "happy-dom": "^20.0.11",
    "sass": "^1.96.0",
    "stylelint": "^16.26.1",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.15",
    "typedoc-plugin-missing-exports": "^4.1.2",
    "typescript": "^5.9.3",
    "typescript-plugin-css-modules": "^5.2.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.10",
    "vue-material-design-icons": "^5.3.1",
    "vue-tsc": "^3.1.8"
  },
  "engines": {
    "node": "^20 || ^22 || ^24"
  },
  "devEngines": {
    "packageManager": [
      {
        "name": "npm",
        "version": "^11.3.0",
        "onFail": "error"
      }
    ],
    "runtime": {
      "name": "node",
      "version": "^24.0.0"
    }
  }
}
